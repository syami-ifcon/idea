<div>
	<%= form_with(url: '/sign_up',:multipart => true) do |form| %>
		<div class="field">
			<%= form.label :name %>
			<%= form.text_field :name %>
		</div>
		
		<div class="field">
			<%= form.label :password %>
			<%= form.password_field :password %>
		</div>

		<div>
			<%= form.submit "Sign Up"%>
		</div>
	<% end %>
</div>